{
  "name": "coming-soon",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format:check": "prettier --ignore-path .gitignore --check .",
    "format:fix": "prettier --ignore-path .gitignore --write .",
    "lint:check": "eslint --ignore-path .gitignore --ext .jsx,.js,.tsx,.ts ./",
    "lint:fix": "npm run lint:check -- --fix",
    "lint:tsc": "tsc --noEmit --project tsconfig.json",
    "prepare": "husky install",
    "test": "jest src",
    "test:watch": "npm run test -- --watch --verbose",
    "remote:restart": "bash ./deploy/restart.sh",
    "remote:destroy": "bash ./deploy/delete.sh",
    "remote:start": "bash ./deploy/start.sh",
    "remote:deploy": "bash ./deploy/pull.sh && bash ./deploy/deploy.sh",
    "deploy": "npm run build && npm run remote:deploy && npm run remote:restart"
  },
  "dependencies": {
    "@hookform/resolvers": "2.9.10",
    "@mailchimp/mailchimp_marketing": "3.0.80",
    "@sentry/nextjs": "^7.20.0",
    "@types/node": "18.11.9",
    "@types/react-dom": "18.0.8",
    "jest": "29.3.1",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.39.1",
    "react-intl": "6.2.1",
    "react-use": "17.4.0",
    "styled-components": "5.3.6",
    "typescript": "4.8.4",
    "uuid": "9.0.0",
    "zod": "3.19.1"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.5.1",
    "@types/mailchimp__mailchimp_marketing": "^3.0.6",
    "@types/react": "^18.0.1",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^8.3.4",
    "eslint": "^8.27.0",
    "eslint-config-next": "^12.2.2",
    "husky": "^8.0.2",
    "next-compose-plugins": "^2.2.1",
    "next-transpile-modules": "^10.0.0",
    "prettier": "^2.7.1",
    "url-loader": "^4.1.1",
    "webpack-bundle-analyzer": "^4.7.0"
  }
}
